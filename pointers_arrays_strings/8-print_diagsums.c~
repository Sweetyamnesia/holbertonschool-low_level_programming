#include "main.h"
#include <stdio.h>

/**
* print_diagsums - function that prints the sum of two diagonals of a square.
* @a: the pointer.
* @size : the size of the square.
**/

void print_diagsums(int *a, int size)
{
int diag1 = 0;
int diag2 = 0;
for (i = 0; i < n; i++)
{
diag1 += *a [i, i];
diag2 += *a [i, n - i - 1];
}
}
