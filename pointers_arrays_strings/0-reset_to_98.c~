#include <stdio.h>
# include "main.h"

/**
* reset_to_98 - Function that takes a pointer to an int as parameter.
* @n: the parameter of the function.
**/

void reset_to_98(int *n)
{
*n = 98;
}

/** 
* main - parameters are passed by value.
* Return: Always 0.
*/

int main(void)
{
int n = 42;
reset_to_98(&n);
return (0);
}
