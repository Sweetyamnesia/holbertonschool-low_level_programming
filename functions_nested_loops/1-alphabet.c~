#include <stdio.h>
#include <stdlib.h>
#include "main.h"
/**
* main - Entry point
* Description : The main function will be called without any parameters.
*Return: Always 0 (Success)
* _putchar - Prints a character to the standard output.
*/

int main(void)
{
char ch;
for (ch = 'a'; ch <= 'z'; ch++)
{
_putchar(ch);
}
_putchar('\n');
return (0);
}
